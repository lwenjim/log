{
    "name": "lwenjim/log",
    "description": "record log",
    "version": "1.0",
    "license": "MIT",
    "require": {
        "ext-json": "^1.5",
        "monolog/monolog": "1.24.0"
    },
    "autoload": {
        "psr-4": {
            "JimLog\\": "src/"
        },
        "files": [
            "common/help.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "JimLog\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "7.5.9"
    },
    "config": {
        "secure-http": false,
        "vendor-dir": "vendor",
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
